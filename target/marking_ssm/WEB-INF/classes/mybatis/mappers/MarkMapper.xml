<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ga.dqrjz.marking.mapper.MarkMapper">
    <select id="selectMarkValueListByFactId" resultType="java.lang.Long">
        select value from mark where fact_id = #{factId} order by mid asc
    </select>
</mapper>